 author = {B. F. Yu and Z. B. Hu and M. Liu and H. L. Yang and Q. X. Kong and Y. H. Liu},
 doi = {10.1016/j.ijrefrig.2008.05.004},
 issn = {01407007},
 issue = {1},
 journal = {International Journal of Refrigeration},
 keywords = {Air conditioning,Air quality,Comfort,Cooling system,Health,Human,Survey,Technology,Ventilation},
 pages = {3-20},
 publisher = {Elsevier Ltd and IIR},
 title = {Review of research on air-conditioning systems and indoor air quality control for human health},
 url = {http://dx.doi.org/10.1016/j.ijrefrig.2008.05.004},
 volume = {32},
 year = {2009}
}
